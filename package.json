{
  "name": "dev2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.2.3",
    "babel-eslint": "^8.2.3",
    "chart.js": "^2.7.2",
    "grid-styled": "^4.1.0",
    "prettier-eslint": "^8.8.1",
    "react": "^15.6.2",
    "react-chartjs-2": "^2.5.1",
    "react-dom": "^15.6.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "recharts": "^1.0.0-beta.10",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "react-scripts": "1.0.10",
    "xo": "^0.21.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "xo",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add",
      "xo"
    ]
  },
  "precommit": {
    "run": [
      "lint-staged"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComa": "none",
    "bracketSpacing": true
  },
  "xo": {
    "parser": "babel-eslint",
    "envs": [
      "brwoser",
      "node"
    ],
    "prettier": true,
    "space": true
  },
  "homepage": "http://daniel40392.github.io/react-portfolio"
}
